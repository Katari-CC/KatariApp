{
  "expo": {
    "name": "Katari",
    "slug": "katari",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_white.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#d0d3c5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/*"],
    "android": {
      "package": "com.codechrysalis.katari",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "versionCode": 3
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codechrysalis.katari"
    },
    "description": "",
    "githubUrl": "https://github.com/Katari-CC/KatariApp"
  }
}
